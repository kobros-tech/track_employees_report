<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id='hr_work_location_form_view_custom' model='ir.ui.view'>
        <field name="name">hr.work.location.view.form.custom</field>
        <field name="model">hr.work.location</field>
        <field name="inherit_id" ref="hr.hr_work_location_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='location_type']" position="after">
                <field name="work_from_home" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
</odoo>

